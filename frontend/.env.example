# Google OAuth (server-side only)
GOOGLE_OAUTH_CLIENT_ID="your-client-id.apps.googleusercontent.com"
GOOGLE_OAUTH_CLIENT_SECRET="your-client-secret"

# Cookie settings (needed in browser for cookie handling)
PUBLIC_COOKIE_SECURE="false" # Set to "true" in production with HTTPS
PUBLIC_COOKIE_SAME_SITE="lax"

# Other settings (needed in browser for redirects)
PUBLIC_APP_URL="http://localhost:5173" # SvelteKit dev server URL

# Backend URL
BACKEND_URL="http://localhost:8000"
